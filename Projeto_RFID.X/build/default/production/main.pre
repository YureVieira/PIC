unsigned char _mode;
inline void SPI_init(unsigned char mode)
{
_mode = mode;
TRISA &= ~(1<<7 | 1<<6);
TRISA |= (1<<5);
if(mode == 2 || mode == 3)
PORTA |= (1<<7);
else
PORTA &= ~(1<<7);
}
unsigned char SPI_transfer(unsigned char data)
{
unsigned char ret;
if(_mode==1 || _mode==3)
{
for(int i=0;i<8;i++)
{

if(data & 0x80)PORTA |= (1<<6);
else PORTA &= ~(1<<6);

PORTA ^= (1<<7);
if(PORTA & (1<<5))ret|=1;
_delay((unsigned long)((10)*(4000000/4000000.0)));
PORTA ^= (1<<7);

ret<<=1;
data<<=1;
_delay((unsigned long)((10)*(4000000/4000000.0)));
}
}
else
{
unsigned char ret;
for(int i=0;i<8;i++)
{

if(PORTA & (1<<5))ret|=1;
PORTA ^= (1<<7);


if(data & 0x80)PORTA |= (1<<6);
else PORTA &= ~(1<<6);
_delay((unsigned long)((10)*(4000000/4000000.0)));
PORTA ^= (1<<7);

ret<<=1;
data<<=1;
_delay((unsigned long)((10)*(4000000/4000000.0)));
}
}
return ret;
}

